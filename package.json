{
  "name": "reflex-virtual-dom-driver",
  "version": "0.2.3",
  "description": "virtual-dom based driver for reflex",
  "keywords": [
    "reflex",
    "react",
    "renderer"
  ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/mozilla/reflex-virtual-dom-driver",
  "main": "./lib/index.js",
  "dependencies": {
    "blanks": "0.0.2",
    "object-as-dictionary": "0.0.4",
    "virtual-dom": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-plugin-remove-comments": "2.0.0",
    "babel-plugin-syntax-flow": "6.3.13",
    "babel-plugin-transform-es2015-modules-umd": "6.4.3",
    "babel-plugin-transform-flow-strip-types": "6.4.0",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.4.3",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "flow-bin": "0.28.0",
    "jsdom": "9.8.0",
    "reflex": "0.4.1",
    "tap": "1.1.0",
    "tape": "4.4.0"
  },
  "scripts": {
    "test": "npm run flow && npm run tape",
    "tape": "tape -r babel-register test/**/*.js",
    "flow": "flow check",
    "start": "babel --watch --out-dir ./lib ./src",
    "build:node": "babel --out-dir ./lib ./src && cp ./src/*.flow ./lib",
    "build:browser": "browserify --require ./src/index:reflex-virtual-dom-driver --debug --transform babelify --outfile dist/reflex-virtual-dom-driver.js",
    "build": "npm run build:node",
    "prepublish": "npm run build && npm run flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/reflex-virtual-dom-driver.git",
    "web": "https://github.com/mozilla/reflex-virtual-dom-driver"
  },
  "bugs": {
    "url": "https://github.com/mozilla/reflex-virtual-dom-driver/issues/"
  },
  "license": "MIT"
}
