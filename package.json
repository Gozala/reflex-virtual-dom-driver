{
  "name": "reflex-virtual-dom-driver",
  "version": "0.2.5",
  "description": "virtual-dom based driver for reflex",
  "keywords": [
    "reflex",
    "react",
    "renderer"
  ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/mozilla/reflex-virtual-dom-driver",
  "main": "./lib/index.js",
  "dependencies": {
    "blanks": "0.0.2",
    "object-as-dictionary": "0.1.0",
    "virtual-dom": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.24.1",
    "flow-bin": "0.48.0",
    "flow-copy-source": "1.1.0",
    "tap": "1.1.0",
    "tape": "4.4.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "jsdom": "9.8.0",
    "reflex": "0.4.1"
  },
  "scripts": {
    "test": "npm run test:flow && npm run test:tape",
    "test:tape": "tape -r babel-register test/**/*.js",
    "test:flow": "flow check",
    "build:clear": "rm -rf ./*.js && rm -rf ./*.js.flow",
    "build:types": "flow-copy-source -v src .",
    "build:node": "babel --out-dir . src",
    "build": "npm run build:node && npm run build:types",
    "prepublish": "npm run build && npm test",
    "start": "babel --watch --out-dir . src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/reflex-virtual-dom-driver.git",
    "web": "https://github.com/mozilla/reflex-virtual-dom-driver"
  },
  "bugs": {
    "url": "https://github.com/mozilla/reflex-virtual-dom-driver/issues/"
  },
  "license": "MIT"
}
